{
    "tags": {
        "allowUnknownTags": true
    },
    "plugins": ["node_modules/jsdoc-strip-async-await"],
    "source": {
        "includePattern": ".+\\.(js(doc)?)|(discord)$",
        "excludePattern": "((^|\\/|\\\\)_)|(\\.json$)",
        "exclude": [
            "node_modules",
            "docs",
            "examples"
        ]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "default": {
            "outputSourceFiles": true
        }
    }
}
